.idea/
.vscode/
*.iml

# VSCode settings
.vscode/settings.json

# Backup files
*~
*.bak
*.swp

# Log files
*.log

# Temporary files
*.tmp
*.temp

# Generated test artifacts
/tests/
*.hprof

